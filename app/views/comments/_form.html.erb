<%= form_with(model: [ @house, @house.comments.build ], local: true) do |form| %>
<% if current_user %>
<p>
    <%= form.label :commenter%><br>
    <%= form.text_field :commenter, :value => current_user.email, :readonly => true%>
</p>
<p>
    <%= form.label :body%>
    <%= form.text_area :body%>
</p>
<p>
    <%= form.submit%>
</p>
<%end%>
<%end%>