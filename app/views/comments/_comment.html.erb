<p>
    <strong>Commenter:</strong>
    <%= comment.commenter%>
</p>
<p>
    <strong>Comment:</strong>
    <%= comment.body%>
</p>

<p>
    <% if current_user && current_user.email == comment.commenter %>
    <%= link_to 'Delete Comment', [comment.house, comment],
                method: :delete,
                data: { confirm: 'Are you sure?' }%>
    <% end %>
</p>