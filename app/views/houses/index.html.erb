<h1>Available Houses</h1>

<table>
    <tr>
        <th>Address</th>
        <th>Monthly Rent</th>
        <th>Available Date</th>
    </tr>
    
    <% @houses.each do |house| %>
    <tr>
        <td><%= house.address %></td>
        <td><%= house.monthly_rent %></td>
        <td><%= house.available_date %></td>
        <td><%= link_to 'Show', house_path(house) %></td>
    </tr>
    <% end %>
</table>

<%= link_to 'New House', new_house_path %>
<%= link_to 'Home', controller: 'home' %>